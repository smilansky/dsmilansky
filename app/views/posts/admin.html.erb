<div class="container">
  <div class="row">
    <div class="col-md-4">
  <%= link_to 'Create Post', new_post_path, :class => 'btn btn-primary' %>
  </div>
  </div>
    <% @posts.each do |post| %>
      <div class="row">
      <div class="well">
        <%= link_to post.title, articles_path(post) %>
        <%= button_to "Delete", post, :method => :delete, :class => "btn btn-danger" %>
      </div>
      </div>
    <% end %>
</div>
